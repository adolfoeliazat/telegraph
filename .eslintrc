{
  "env": {
    "es6": true,
    "browser": true
  },

  "extends": "eslint-config-airbnb",

  globals: {
    "clients": true,
    "fetch": true,
    "self": true
  },

  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
    "no-console": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-useless-escape": 0,
    "prefer-const": 0
  }
}
